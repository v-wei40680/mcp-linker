{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "fs:default",
    "fs:default",
    "fs:default",
    "opener:default",
    "shell:default",
    "dialog:default",
    "os:default",
    "deep-link:default",
    "updater:default",
    "clipboard-manager:allow-read-text",
    "core:window:default",
    "core:window:allow-start-dragging",
    "fs:default",
    {
      "identifier": "fs:allow-appcache-read"
    },
    {
      "identifier": "fs:allow-appcache-write"
    }
  ]
}